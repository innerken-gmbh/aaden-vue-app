{
  "name": "aaden-vue-app",
  "version": "0.3.29",
  "private": true,
  "description": "全新构建的Aaden Vue版本，重新启航，更加牢靠！",
  "author": "juhaodong@gmail.com",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build --linux snap --publish always",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "release": "sudo snap install snapcraft --classic && snapcraft login --with log && vue-cli-service electron:build --linux snap --publish always",
    "update": "dotenv -e .env.local -- release-it --ci --no-npm-publish"
  },
  "main": "background.js",
  "dependencies": {
    "aaden-base-model": "^0.1.76",
    "core-js": "^3.6.5",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.3.1",
    "innerken-utils": "^1.7.28",
    "lodash-es": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "register-service-worker": "^1.7.1",
    "sweetalert2": "^9.15.1",
    "vue": "^2.6.11",
    "vue-dragscroll": "^2.1.0",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@mdi/font": "^5.5.55",
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.0",
    "@vue/cli-plugin-vuex": "^4.4.4",
    "@vue/cli-service": "^4.4.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "dotenv-cli": "^3.1.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "release-it": "^13.5.8",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.2",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/innerken-gmbh/aaden-vue-app/issues"
  },
  "homepage": "http://aaden.online",
  "release-it": {
    "git": {
      "commitMessage": "update: release v${version}",
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innerken-gmbh/aaden-vue-app.git"
  }
}
