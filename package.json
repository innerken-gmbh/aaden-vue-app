    "git": {
      "commitMessage": "[VersionChange]:: release v${version}",
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innerken-gmbh/aaden-vue-app.git"
  }
